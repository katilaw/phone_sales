<h1>Add a new manufacturer</h1>
  <%= form_for @manufacturer do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :country %>
    <%= f.text_area :country %>
    <%= f.submit 'Create Manufacturer' %>
  <% end %>
<%= link_to 'Return to Manufacturer Index', manufacturers_path %>
